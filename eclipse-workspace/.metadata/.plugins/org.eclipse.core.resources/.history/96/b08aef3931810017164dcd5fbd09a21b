package com.jogamp.opengl.math;























public class Ray
{
  public Ray() {}
  






















  public final float[] orig = new float[3];
  

  public final float[] dir = new float[3];
  
  public String toString() {
    return "Ray[orig[" + orig[0] + ", " + orig[1] + ", " + orig[2] + "], dir[" + dir[0] + ", " + dir[1] + ", " + dir[2] + "]]";
  }
}
