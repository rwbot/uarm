package com.jogamp.opengl.util.gl2;











class StrokeCharRec
{
  public int num_strokes;
  








  public StrokeRec[] stroke;
  








  public float center;
  








  public float right;
  









  public StrokeCharRec(int paramInt, StrokeRec[] paramArrayOfStrokeRec, float paramFloat1, float paramFloat2)
  {
    num_strokes = paramInt;
    stroke = paramArrayOfStrokeRec;
    center = paramFloat1;
    right = paramFloat2;
  }
}
