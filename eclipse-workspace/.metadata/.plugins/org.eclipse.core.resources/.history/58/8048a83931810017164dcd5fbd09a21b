    if (!openShape) {
      PGraphics.showWarning("%1$s can only be called between beginShape() and endShape()", new Object[] { "stroke()" });
      return;
    }
    
    stroke = true;
    colorCalc(rgb, alpha);
    strokeColor = calcColor;
  }
  
