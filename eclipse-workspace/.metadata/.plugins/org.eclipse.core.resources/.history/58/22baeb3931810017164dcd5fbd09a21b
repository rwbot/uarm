package com.jogamp.opengl;

import java.util.EventListener;

public abstract interface GLEventListener
  extends EventListener
{
  public abstract void init(GLAutoDrawable paramGLAutoDrawable);
  
  public abstract void dispose(GLAutoDrawable paramGLAutoDrawable);
  
  public abstract void display(GLAutoDrawable paramGLAutoDrawable);
  
  public abstract void reshape(GLAutoDrawable paramGLAutoDrawable, int paramInt1, int paramInt2, int paramInt3, int paramInt4);
}
