package com.jogamp.opengl;

import java.nio.Buffer;

public abstract interface GLArrayData
{
  public abstract void associate(Object paramObject, boolean paramBoolean);
  
  public abstract boolean isVertexAttribute();
  
  public abstract int getIndex();
  
  public abstract String getName();
  
  public abstract void setName(String paramString);
  
  public abstract int getLocation();
  
  public abstract int setLocation(int paramInt);
  
  public abstract int setLocation(GL2ES2 paramGL2ES2, int paramInt);
  
  public abstract int setLocation(GL2ES2 paramGL2ES2, int paramInt1, int paramInt2);
  
  public abstract boolean isVBO();
  
  public abstract long getVBOOffset();
  
  public abstract int getVBOName();
  
  public abstract int getVBOUsage();
  
  public abstract int getVBOTarget();
  
