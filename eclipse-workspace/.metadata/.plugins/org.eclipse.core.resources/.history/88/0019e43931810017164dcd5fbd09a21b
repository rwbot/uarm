package com.jogamp.nativewindow.util;

import com.jogamp.common.type.WriteCloneable;

public abstract interface InsetsImmutable
  extends WriteCloneable
{
  public abstract int getLeftWidth();
  
  public abstract int getRightWidth();
  
  public abstract int getTotalWidth();
  
  public abstract int getTopHeight();
  
  public abstract int getBottomHeight();
  
  public abstract int getTotalHeight();
  
  public abstract boolean equals(Object paramObject);
  
  public abstract int hashCode();
}
