package jogamp.opengl;

import com.jogamp.common.os.DynamicLibraryBundle;



























public class GLDynamicLookupHelper
  extends DynamicLibraryBundle
{
  public GLDynamicLookupHelper(GLDynamicLibraryBundleInfo paramGLDynamicLibraryBundleInfo)
  {
    super(paramGLDynamicLibraryBundleInfo);
  }
  
  public final GLDynamicLibraryBundleInfo getGLBundleInfo() { return (GLDynamicLibraryBundleInfo)getBundleInfo(); }
  
  public boolean loadGLULibrary() {
    return false;
  }
}
