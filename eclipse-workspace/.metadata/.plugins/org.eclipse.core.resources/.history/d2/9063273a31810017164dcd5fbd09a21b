package jogamp.opengl.glu.nurbs;












public class O_nurbscurve
{
  public Quilt bezier_curves;
  










  public int type;
  









  public boolean used;
  









  public O_curve owner;
  









  public O_nurbscurve next;
  










  public O_nurbscurve(int paramInt)
  {
    type = paramInt;
    owner = null;
    next = null;
    used = false;
  }
}
