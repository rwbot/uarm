package com.jogamp.opengl.util;

import com.jogamp.opengl.GLAutoDrawable;
import com.jogamp.opengl.GLEventListener;

public abstract interface CustomGLEventListener
  extends GLEventListener
{
  public static final int DISPLAY_REPEAT = 1;
  public static final int DISPLAY_DONTCLEAR = 2;
  
  public abstract void display(GLAutoDrawable paramGLAutoDrawable, int paramInt);
}
