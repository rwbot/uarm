package com.jogamp.nativewindow;

public abstract interface AbstractGraphicsConfiguration
  extends VisualIDHolder, Cloneable
{
  public abstract Object clone();
  
  public abstract AbstractGraphicsScreen getScreen();
  
  public abstract CapabilitiesImmutable getChosenCapabilities();
  
  public abstract CapabilitiesImmutable getRequestedCapabilities();
  
  public abstract AbstractGraphicsConfiguration getNativeGraphicsConfiguration();
}
