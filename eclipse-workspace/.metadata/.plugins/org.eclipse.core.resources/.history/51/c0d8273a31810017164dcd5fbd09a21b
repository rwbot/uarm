package jogamp.opengl.glu.nurbs;










public class Quiltspec
{
  public int stride;
  








  public int width;
  








  public int order;
  








  public int offset;
  







  public int index;
  







  public int[] bdry;
  







  public float[] breakpoints;
  








  public Quiltspec()
  {
    bdry = new int[2];
  }
}
