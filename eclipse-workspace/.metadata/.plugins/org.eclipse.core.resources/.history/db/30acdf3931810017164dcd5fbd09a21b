package com.jogamp.graph.curve.tess;

import com.jogamp.graph.geom.Outline;
import com.jogamp.graph.geom.Triangle;
import java.util.List;

public abstract interface Triangulator
{
  public abstract void addCurve(List<Triangle> paramList, Outline paramOutline, float paramFloat);
  
  public abstract void generate(List<Triangle> paramList);
  
  public abstract void reset();
  
  public abstract int getAddedVerticeCount();
}
