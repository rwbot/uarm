package com.jogamp.opengl.glu;

import com.jogamp.opengl.GL;
import com.jogamp.opengl.util.ImmModeSink;

public abstract interface GLUquadric
{
  public abstract void enableImmModeSink(boolean paramBoolean);
  
  public abstract boolean isImmModeSinkEnabled();
  
  public abstract void setImmMode(boolean paramBoolean);
  
  public abstract boolean getImmMode();
  
  public abstract ImmModeSink replaceImmModeSink();
  
  public abstract void resetImmModeSink(GL paramGL);
}
