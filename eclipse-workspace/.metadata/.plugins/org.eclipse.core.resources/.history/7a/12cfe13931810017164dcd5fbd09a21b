    super(paramString);
  }
  

  public NativeWindowException(String paramString, Throwable paramThrowable)
  {
    super(paramString, paramThrowable);
  }
  

  public NativeWindowException(Throwable paramThrowable)
  {
    super(paramThrowable);
  }
}
