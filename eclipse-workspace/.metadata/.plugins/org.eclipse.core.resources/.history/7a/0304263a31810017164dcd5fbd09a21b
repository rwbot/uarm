package jogamp.opengl.glu.mipmap;

import java.io.PrintStream;
import java.nio.ByteBuffer;


















































public class Type_Widget
{
  ByteBuffer buffer;
  
  public Type_Widget()
  {
    buffer = ByteBuffer.allocate(4);
  }
  
  public void setUB0(byte paramByte) {
    buffer.position(0);
    buffer.put(paramByte);
  }
  
  public byte getUB0() {
    buffer.position(0);
    return buffer.get();
  }
  
  public void setUB1(byte paramByte) {
    buffer.position(1);
    buffer.put(paramByte);
  }
  
  public byte getUB1() {
    buffer.position(1);
    return buffer.get();
  }
  
  public void setUB2(byte paramByte) {
    buffer.position(2);
    buffer.put(paramByte);
  }
  
  public byte getUB2() {
    buffer.position(2);
    return buffer.get();
  }
  
  public void setUB3(byte paramByte) {
    buffer.position(3);
    buffer.put(paramByte);
  }
  
  public byte getUB3() {
    buffer.position(3);
    return buffer.get();
  }
  
  public void setUS0(short paramShort) {
    buffer.position(0);
    buffer.putShort(paramShort);
  }
  
  public short getUS0() {
    buffer.position(0);
    return buffer.getShort();
  }
  
  public void setUS1(short paramShort) {
    buffer.position(2);
    buffer.putShort(paramShort);
  }
  
  public short getUS1() {
    buffer.position(2);
    return buffer.getShort();
  }
  
  public void setUI(int paramInt) {
    buffer.position(0);
    buffer.putInt(paramInt);
  }
  
  public int getUI() {
    buffer.position(0);
    return buffer.getInt();
  }
  
  public void setB0(byte paramByte) {
    buffer.position(0);
    buffer.put(paramByte);
  }
  
  public byte getB0() {
    buffer.position(0);
    return buffer.get();
  }
  
  public void setB1(byte paramByte) {
    buffer.position(1);
    buffer.put(paramByte);
  }
  
  public byte getB1() {
    buffer.position(1);
    return buffer.get();
  }
  
  public void setB2(byte paramByte) {
    buffer.position(2);
    buffer.put(paramByte);
  }
  
  public byte getB2() {
    buffer.position(2);
    return buffer.get();
  }
  
  public void setB3(byte paramByte) {
    buffer.position(3);
    buffer.put(paramByte);
  }
  
  public byte getB3() {
    buffer.position(3);
    return buffer.get();
  }
  
  public void setS0(short paramShort) {
    buffer.position(0);
    buffer.putShort(paramShort);
  }
  
  public short getS0() {
    buffer.position(0);
    return buffer.getShort();
  }
  
  public void setS1(short paramShort) {
    buffer.position(2);
    buffer.putShort(paramShort);
  }
  
  public short getS1() {
    buffer.position(2);
    return buffer.getShort();
  }
  
  public void setI(int paramInt) {
    buffer.position(0);
    buffer.putInt(paramInt);
  }
  
  public int getI() {
    buffer.position(0);
    return buffer.getInt();
  }
  
  public void setF(float paramFloat) {
    buffer.position(0);
    buffer.putFloat(paramFloat);
  }
  
  public float getF() {
    buffer.position(0);
    return buffer.getFloat();
  }
  
  public ByteBuffer getBuffer() {
    buffer.rewind();
    return buffer;
  }
  
  public static void main(String[] paramArrayOfString) {
    Type_Widget localType_Widget = new Type_Widget();
    localType_Widget.setI(1000000);
    
    System.out.println("int: " + Integer.toHexString(localType_Widget.getI()));
  }
}
