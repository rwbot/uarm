package jogamp.opengl.glu.nurbs;















public class PwlArc
{
  private final int npts;
  













  public TrimVertex[] pts;
  













  private final int type;
  













  public PwlArc(int paramInt, TrimVertex[] paramArrayOfTrimVertex)
  {
    npts = paramInt;
    pts = paramArrayOfTrimVertex;
    type = 8;
  }
}
