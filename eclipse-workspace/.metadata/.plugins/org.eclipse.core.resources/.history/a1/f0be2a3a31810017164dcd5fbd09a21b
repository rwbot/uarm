  public static VideoPixelFormat valueOf(int paramInt)
    throws IllegalArgumentException
  {
    VideoPixelFormat[] arrayOfVideoPixelFormat = values();
    if ((0 <= paramInt) && (paramInt < arrayOfVideoPixelFormat.length)) {
      return arrayOfVideoPixelFormat[paramInt];
    }
    throw new IllegalArgumentException("Ordinal " + paramInt + " out of range of PixelFormat.values()[0.." + (arrayOfVideoPixelFormat.length - 1) + "]");
  }
